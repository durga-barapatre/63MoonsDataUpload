	<connectionStrings>
		<add name="DataUploadConnection" connectionString="Data Source=.;Initial Catalog=DataUpload;Integrated Security=True" providerName="System.Data.SqlClient" />
	</connectionStrings>
	<entityFramework>
		<providers>
			<provider invariantName="System.Data.SqlClient" type="System.Data.Entity.SqlServer.SqlProviderServices, EntityFramework.SqlServer" />
